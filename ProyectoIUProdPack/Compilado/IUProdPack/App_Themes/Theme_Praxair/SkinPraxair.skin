         <asp:CustomValidator runat="server" Font-Names="Verdana,  Times New Roman, Helvetica, sans-serif" Font-Size="7pt" ForeColor="Red"></asp:CustomValidator>
         <asp:RangeValidator runat="server" Font-Names="Verdana,  Times New Roman, Helvetica, sans-serif" Font-Size="7pt" ForeColor="Red"></asp:RangeValidator>    
