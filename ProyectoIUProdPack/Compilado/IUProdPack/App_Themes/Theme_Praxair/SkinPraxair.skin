        </asp:RadioButtonList>
        
<%-- B E G I N   VALIDATOR --%>           
         <asp:CompareValidator runat="server" Font-Names="Verdana,  Times New Roman, Helvetica, sans-serif" Font-Size="7pt" ForeColor="Red"></asp:CompareValidator>
