<%-- B E G I N   GRIDVIEW ASP.NET --%>
    
      <asp:GridView runat="server" CaptionAlign="Bottom" CellPadding="1" Font-Names="Verdana" Font-Size="8pt" 
            ForeColor="#333333" Width="85%" HorizontalAlign="Center">
			 <EmptyDataTemplate>
                No hay información para esta consulta.
            </EmptyDataTemplate>
            <PagerSettings Mode="NextPrevious" NextPageText="Siguiente" PreviousPageText="Anterior" />
            <RowStyle Font-Names="Verdana" Font-Size="8pt" ForeColor="#333333" Height="15px" HorizontalAlign="Center" VerticalAlign="Middle"  BackColor="White"/>
            <PagerStyle BackColor="Silver" Font-Names="Verdana" Font-Overline="True" Font-Size="8pt" Font-Underline="True" 
                ForeColor="#333333" HorizontalAlign="Center" VerticalAlign="Middle" />
            <SelectedRowStyle BackColor="#6699CC" BorderStyle="Double" ForeColor="#333333" />
            <HeaderStyle CssClass="Header_Grid" />
            <AlternatingRowStyle BackColor="#D2D2D2" Font-Names="Verdana" Font-Size="8pt" ForeColor="#333333" 
                Height="15px" HorizontalAlign="Center" VerticalAlign="Middle" />
        </asp:GridView>

        <asp:GridView runat="server" CaptionAlign="Bottom" CellPadding="0" Font-Names="Verdana" Font-Size="7pt" SkinID="Asp_Grid-Pequeño"
            ForeColor="#333333" Height="100%" Width="85%" HorizontalAlign="Left">
            <EmptyDataTemplate>
                No hay información para esta consulta.
            </EmptyDataTemplate>
            <RowStyle Font-Names="Verdana" Font-Size="7pt" HorizontalAlign="Left" VerticalAlign="Middle" ForeColor="#333333" />
            <PagerStyle BackColor="Silver" Font-Names="Verdana" Font-Overline="True" Font-Size="8pt" Font-Underline="True" 
                ForeColor="#333333" HorizontalAlign="Center" VerticalAlign="Middle" />
            <SelectedRowStyle BorderStyle="Double" ForeColor="#333333" />
            <HeaderStyle CssClass="Header_Grid" />
            <AlternatingRowStyle Font-Names="Verdana" Font-Size="7pt" ForeColor="#333333" 
                HorizontalAlign="Left" VerticalAlign="Middle" />
        </asp:GridView>

        <asp:GridView runat="server" Width="100%"   CellPadding="0" CellSpacing="0" BorderStyle="None" BorderWidth="0px" SkinID="Asp_Grid-ListaTrabajo">
            <EmptyDataTemplate>
                No hay información para esta consulta.
            </EmptyDataTemplate>
            <PagerSettings Mode="Numeric" Position="Top" />
            <PagerStyle BackColor="Silver" Font-Names="Verdana" Font-Overline="True" Font-Size="8pt" Font-Underline="True" 
                ForeColor="#333333" HorizontalAlign="Center" VerticalAlign="Middle" />
            <AlternatingRowStyle  Font-Names="Verdana" Font-Size="7pt" Height="27px" VerticalAlign="Middle" HorizontalAlign="Left" CssClass="RowStyleLista" BackColor="White"/>
            <RowStyle Font-Names="Verdana" Font-Size="7pt" Height="27px" VerticalAlign="Middle" HorizontalAlign="Left" CssClass="RowStyleLista"/>
        </asp:GridView>
        