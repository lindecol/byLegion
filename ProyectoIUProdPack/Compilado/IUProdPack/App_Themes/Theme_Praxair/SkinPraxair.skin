            Font-Size="11px" Font-Strikeout="False" ForeColor="#636500" Width="85%">
        </asp:TextBox>	

<%-- B E G I N   CHECKBOX CHECKBOXLIST--%>   
        <asp:CheckBox runat="server" Font-Names="Verdana,  Times New Roman, Helvetica, sans-serif" Font-Size="11px" ForeColor="#636500" />
        <asp:CheckBoxList runat="server" CellPadding="1" 
